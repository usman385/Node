<style>
#customers {
  font-family: Arial, Helvetica, sans-serif;
  border-collapse: collapse;
  width: 100%;
  display: flex;
  justify-content: center;
}

#customers td, #customers th {
  border: 1px solid #ddd;
  padding: 8px;
}

#customers tr:nth-child(even){background-color: #f2f2f2;}

#customers tr:hover {background-color: #ddd;}

#customers th {
  padding-top: 12px;
  padding-bottom: 12px;
  text-align: left;
  background-color: #4CAF50;
  color: white;
}
#heading{
     padding-top: 12px;
  padding-bottom: 12px;
  text-align: center;
  background-color: #4CAF50;
  color: white;
  margin-top: 18px;
  display: flex;
  justify-content: center;
  width: 50%;
  margin-left: 16rem;
  border-radius: 3px;
}

#update{
    background-color: #4CAF50;
    color: white;
    border-radius: 10px;
    border: #4CAF50;

}


input[type=text], select {
  width: 50%;
  padding: 12px 20px;
  margin: 8px 0;
  display: inline-block;
  border: 1px solid #ccc;
  border-radius: 4px;
  box-sizing: border-box;
  display: flex;
   margin-left: 10rem;
  
}
input[type=email], select {
  width: 50%;
  padding: 12px 20px;
  margin: 8px 0;
  display: inline-block;
  border: 1px solid #ccc;
  border-radius: 4px;
  box-sizing: border-box;
  display: flex;
   margin-left: 10rem;
}
input[type=number], select {
  width: 50%;
  padding: 12px 20px;
  margin: 8px 0;
  display: inline-block;
  border: 1px solid #ccc;
  border-radius: 4px;
  box-sizing: border-box;
  display: flex;
   margin-left: 10rem;
}

input[type=submit] {
  width: 50%;
  background-color: #4CAF50;
  color: white;
  padding: 14px 20px;
  margin: 8px 0;
  border: none;
  border-radius: 4px;
  cursor: pointer;
   margin-left: 10rem;
}

input[type=submit]:hover {
  background-color: #45a049;
}

#formik {
  border-radius: 5px;
 
  padding: 20px;
  display: flex;
  flex-direction: column;
  margin-left: 20rem;
  margin-right: 20rem;
  
  
}
</style>





<h1 id="heading">{{title}}</h1>




<table id="customers">
    <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Age</th>
    <th>Status</th>
    <th>Update</th>
    <th>Delete</th>
</tr>
    {{#each members}}

<tr>
    <td>{{this.name}}</td>
    <td>{{this.email}}</td>
    <td>{{this.age}}</td>
    <td>{{this.status}}</td>
    <td ><button  id="update" >Update</button></td>
    <td><button id="update">Delete</button></td>

</tr>
{{/each}}
</table>